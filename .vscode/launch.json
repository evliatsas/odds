  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Consumer",
      "program": "${workspaceFolder}/smq/src/queue\\consumer.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Archive Consumer",
      "program": "${workspaceFolder}/smq/src/archive\\consumer.js"
    }
  ]
}
